package editor.utils{		public class Calculator {				public static function bAngle(a:Number):Number{			return Number((a * Math.PI / 180).toFixed(3));		}		public static function fAngle(a:Number):Number{			return Number((a / Math.PI * 180).toFixed(3));		}		public static function b2dSize2DArray(a:Array):Array{			for (var i:int = 0; i<a.length; i++){				 				for (var j:int = 0; j<a[i].length; j++){					a[i][j] = b2dSize(a[i][j]);				}			}			return a;		}		public static function flaSize2DArray(a:Array):Array{			for (var i:int = 0; i<a.length; i++){				 				for (var j:int = 0; j<a[i].length; j++){					a[i][j] = flaSize(a[i][j]);				}			}			return a;		}		public static function b2dSize(n:Number):Number{			n /= 30;			var s:String = n.toFixed(2);			return Number(s);		}		public static function flaSize(n:Number):Number{			n *= 30;			var s:String = n.toFixed(2);			return Number(s);		}	}}