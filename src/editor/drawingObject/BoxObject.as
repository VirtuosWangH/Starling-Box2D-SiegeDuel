package editor.drawingObject{	import editor.utils.Calculator;
		public class BoxObject extends BaseObject {		 		public function BoxObject(type:String=""){			 super(type);		}		     	override public function get objectDef():String{			data = {x:Calculator.b2dSize(x), y:Calculator.b2dSize(y), 					width:Calculator.b2dSize(shapeData.width), height:Calculator.b2dSize(shapeData.height), 					angle: Calculator.bAngle(rotation), 					density:shapeData.density};			return  preCommandStr() + "sim.addBox({x:" + data.x + ", y:" + data.y 					+ ", width:" + data.width + ", height:" + data.height 					+ ", angle: "+ data.angle + ", density:"+ data.density+"});";		}				override public function createFromObject(nm:String, obj:Object):void{			if (nm != ""){				name = nm;			}			var w:Number = Calculator.flaSize(obj.width);			var h:Number = Calculator.flaSize(obj.height);			x = Calculator.flaSize(obj.x);			y = Calculator.flaSize(obj.y);			 			graphics.lineStyle(0x000000);			graphics.beginFill(0xCCCCCC);			graphics.drawRect(-w/2, -h/2, w, h);			rotation = Calculator.fAngle(obj.angle);						shapeData.width = w			shapeData.height = h;			shapeData.density = obj.density;		}	}}